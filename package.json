{
  "name": "settlers.ts",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test:unit": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "analyze:circular": "madge --circular --extensions ts src/",
    "analyze:type-coverage": "type-coverage --project tsconfig.json --detail --strict",
    "analyze:unused": "knip",
    "analyze:duplication": "jscpd src",
    "analyze:deps": "depcruise src --config .dependency-cruiser.cjs",
    "analyze:utils": "tsx scripts/analyze-utils.ts",
    "analyze": "pnpm analyze:circular && pnpm analyze:deps && pnpm analyze:unused && pnpm analyze:duplication && pnpm analyze:utils",
    "gfx:export": "tsx --tsconfig tsconfig.json scripts/gfx-export/cli.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@vueuse/core": "^14.2.0",
    "fengari": "^0.1.5",
    "fengari-interop": "^0.1.4",
    "howler": "^2.2.4",
    "vue": "^3.4.0",
    "vue-router": "^5.0.0",
    "wasmoon": "^1.16.0",
    "yaml": "^2.8.2"
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint --fix"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@rollup/pluginutils": "^5.3.0",
    "@types/howler": "^2.2.12",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^25.2.0",
    "typescript-eslint": "^8.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/ui": "^2.1.9",
    "@vue/test-utils": "^2.4.0",
    "dependency-cruiser": "^16.0.0",
    "@eslint/js": "^9.0.0",
    "eslint": "^9.0.0",
    "eslint-import-resolver-typescript": "^4.4.0",
    "eslint-plugin-import-x": "^4.0.0",
    "globals": "^16.0.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "eslint-plugin-vue": "^10.0.0",
    "vue-eslint-parser": "^10.0.0",
    "husky": "^9.1.7",
    "jscpd": "^4.0.0",
    "jsdom": "^24.0.0",
    "knip": "^5.0.0",
    "lint-staged": "^16.2.7",
    "madge": "^8.0.0",
    "tsx": "^4.7.0",
    "type-coverage": "^2.29.7",
    "typescript": "~5.9.3",
    "vite": "^5.0.0",
    "vite-plugin-glsl": "^1.3.0",
    "vite-plugin-node-polyfills": "^0.25.0",
    "vitest": "^2.1.9",
    "vue-tsc": "^3.0.0"
  }
}
